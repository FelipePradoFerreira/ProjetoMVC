@model Frekisa.Models.Contrato

@{
    ViewBag.Title = "Excluir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2><b>Excluir</b></h2>

<h3>Tem certeza que quer excluir esse contrato?</h3>
<div class="jumbotron">
    <h4>Contrato</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Clientes.id_cliente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Clientes.id_cliente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Clientes.nome_cliente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Clientes.nome_cliente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Funcionarios.id_funcionario)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Funcionarios.id_funcionario)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Funcionarios.nome_funcionario)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Funcionarios.nome_funcionario)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Plano)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Plano)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger col-md-3" />
            <a class="btn btn-primary" role="button" href="@Url.Action("Index")">Voltar</a>
        </div>
    }
</div>
